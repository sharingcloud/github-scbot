@startuml Schema
class Repository {
    #id: Integer
    name: String
    owner: String
    pr_title_validation_regex: String
}
class PullRequest {
    #id: Integer
    *repository_id: Integer
    number: Integer
    name: String
    automerge: Boolean
    step: String
    check_status: String
    status_comment_id: Integer
    qa_status: String
    wip: Boolean
    required_reviewers: String
}

Repository *-- PullRequest : has
@enduml
